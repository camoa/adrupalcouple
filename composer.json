{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^2.0",
        "drupal/address": "^2.0",
        "drupal/admin_toolbar": "^3.4",
        "drupal/allowed_formats": "^3.0",
        "drupal/auto_entitylabel": "^3.0",
        "drupal/config_overlay": "^2.3",
        "drupal/core-composer-scaffold": "^11",
        "drupal/core-project-message": "^11",
        "drupal/core-recommended": "^11",
        "drupal/custom_field": "^3.0@RC",
        "drupal/devel": "^5.1",
        "drupal/dropdown_language": "^4.0",
        "drupal/editoria11y": "^2.1",
        "drupal/entity_browser": "^2.9",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/field_group": "^3.4",
        "drupal/focal_point": "^2.0",
        "drupal/geolocation": "4.x-dev@dev",
        "drupal/gin": "^3.0@rc",
        "drupal/gin_toolbar": "^1.0@rc",
        "drupal/google_tag": "^2.0",
        "drupal/http_cache_control": "^2.1",
        "drupal/inline_entity_form": "^3.0@RC",
        "drupal/jquery_ui_draggable": "^2.1",
        "drupal/jsonapi_extras": "^3.26@beta",
        "drupal/layout_builder_styles": "^2.1",
        "drupal/layout_paragraphs": "^2.0",
        "drupal/link_attributes": "^2.1",
        "drupal/linkit": "^7.0@alpha",
        "drupal/manage_display": "3.x-dev@dev",
        "drupal/metatag": "^2.0",
        "drupal/moderation_dashboard": "^3",
        "drupal/moderation_sidebar": "^1.7",
        "drupal/node_read_time": "^1.15",
        "drupal/paragraphs": "^1.15",
        "drupal/pathauto": "^1.11",
        "drupal/purge": "^3.5",
        "drupal/redirect": "^1.9",
        "drupal/redirect_404": "^1.9",
        "drupal/schemadotorg": "1.0.x-dev@dev",
        "drupal/seckit": "^2.0",
        "drupal/security_review": "^3",
        "drupal/seo_checklist": "^5.2",
        "drupal/simple_sitemap": "^4.1",
        "drupal/smart_date": "^4.0",
        "drupal/upgrade_status": "^4.3",
        "drupal/yoast_seo": "^2",
        "drush/drush": "^13"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            },
            "file-mapping": {
                "[web-root]/sites/default/default.settings.php": false,
                "[web-root]/.htaccess": false
            }
        },
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
